#!/home/raymond/backup/raymond/mithray/bashba/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'Radioactive-Terminal','console_scripts','hello'
__requires__ = 'Radioactive-Terminal'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Radioactive-Terminal', 'console_scripts', 'hello')()
    )
